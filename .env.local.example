# Mini Sudoku Environment Configuration
# Copy this file to .env.local and update with your values

# Application Mode: 'dev' or 'production'
# - dev: Clean sudoku game only (no auth, no dashboard)
# - production: Full app with authentication and dashboard
VITE_APP_MODE=dev

# After editing this file, run: npm run build:config
# Or just run: npm start (it will auto-build config)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=